@model IEnumerable<nomina.Models.NomUserViewModel>
@{
    ViewBag.Title = "NomUserList";
}

<h2>Nómina con fecha @ViewBag.Fecha</h2>

<p>
    @Html.ActionLink("Back to List", "List")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.ActionLink("Consultar Nomina", "ConsultarUser", new { idNom = item.IdNom, idUser = item.IdUser })
            </td>
        </tr>
    }

</table>
