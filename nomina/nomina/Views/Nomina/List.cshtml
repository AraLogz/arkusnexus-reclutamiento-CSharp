@model IEnumerable<nomina.Models.NominaModel>

@{
    ViewBag.Title = "List";
}

<h2>Nómina</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Mes)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Mes)
        </td>
        <td>
            @Html.ActionLink("Consultar", "NomUserList", new { id=item.Id })
        </td>
    </tr>
}

</table>
